{
    "name": "jdavidbakr/mail-throttler",
    "type": "library",
    "description": "Service Provider to add the Swift ThrottlerPlugin to outgoing mail.",
    "keywords": [
        "jdavidbakr",
        "mail-throttler"
    ],
    "homepage": "https://github.com/jdavidbakr/mail-throttler",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Baker",
            "email": "me@jdavidbaker.com",
            "homepage": "http://www.jdavidbaker.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\MailThrottler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\MailThrottler\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
